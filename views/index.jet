{{extends "layouts/base.jet"}}

{{block body()}}
  <main>
    {{range zone, trucks := spots}}
        <div class="zone">
          <div class="zone__header">
            <h1>{{zone}}</h1>
          </div>
          {{range spot := trucks}}
              {{include "partials/_truck.jet" spot}}
          {{end}}
        </div>
    {{end}}
  </main>
{{end}}